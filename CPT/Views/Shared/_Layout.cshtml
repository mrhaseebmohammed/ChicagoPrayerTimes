    <style>
.offset
{
	margin-top:15px;
}

@@font-face {
  font-family: 'digital-7';
  src: url('../../Content/digital-7.ttf')  format('truetype')
}

body{
  font-size: 2.9em;
}

.digital {
  font-family: 'digital-7', Fallback, sans-serif;
  font-weight: 700;
  font-size: 3vh;
}

th{
background-color: #B4DFE5;
}

.odd{
background-color: #97CA2D;
}

.even{
background-color: #4BB5C2;
}

.white-text{
color: #ffffff;
font-weight: bold;
font-family: sans-serif;
}

.black-text{
color: #000000
}

p{
font-size:1.5vh;}

