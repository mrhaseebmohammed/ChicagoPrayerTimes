        <h1>Chicago Prayer Times</h1>
        <table class="table table-striped table-hover offset">
            <thead>
                <tr>
                    <th>Masjid</th>
                    <th>Fajr</th>
                    <th>Zuhr</th>
                    <th>Asr</th>
                    <th>Maghrib</th>
                    <th>Isha</th>
                    <th>Jumuah</th>
                </tr>
            </thead>
            <tbody>
                @RenderBody()
            </tbody>
        </table>
    </div>

